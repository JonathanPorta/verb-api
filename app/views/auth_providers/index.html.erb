<h1>Listing auth_providers</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Provider</th>
      <th>Uid</th>
      <th>Nickname</th>
      <th>Token</th>
      <th>Secret</th>
      <th>Token expiration</th>
      <th>Last used</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @auth_providers.each do |auth_provider| %>
      <tr>
        <td><%= auth_provider.user_id %></td>
        <td><%= auth_provider.provider %></td>
        <td><%= auth_provider.uid %></td>
        <td><%= auth_provider.nickname %></td>
        <td><%= auth_provider.token %></td>
        <td><%= auth_provider.secret %></td>
        <td><%= auth_provider.token_expiration %></td>
        <td><%= auth_provider.last_used %></td>
        <td><%= link_to 'Show', auth_provider %></td>
        <td><%= link_to 'Edit', edit_auth_provider_path(auth_provider) %></td>
        <td><%= link_to 'Destroy', auth_provider, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Auth provider', new_auth_provider_path %>
